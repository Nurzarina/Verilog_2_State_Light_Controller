#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x55d0e5b2d540 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55d0e5b2d6d0 .scope module, "light_controller_tb" "light_controller_tb" 3 1;
 .timescale 0 0;
v0x55d0e5b543d0_0 .var "button", 0 0;
v0x55d0e5b54490_0 .var "clk", 0 0;
v0x55d0e5b54530_0 .net "light", 0 0, v0x55d0e5b540a0_0;  1 drivers
v0x55d0e5b54600_0 .var "reset", 0 0;
S_0x55d0e5b2d860 .scope module, "uut" "light_controller" 3 8, 4 1 0, S_0x55d0e5b2d6d0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "button";
    .port_info 3 /OUTPUT 1 "light";
enum0x55d0e5af0b80 .enum4 (2)
   "OFF" 2'b00,
   "ON" 2'b01
 ;
v0x55d0e5aefcf0_0 .net "button", 0 0, v0x55d0e5b543d0_0;  1 drivers
v0x55d0e5b53f20_0 .net "clk", 0 0, v0x55d0e5b54490_0;  1 drivers
v0x55d0e5b53fe0_0 .var "current_state", 1 0;
v0x55d0e5b540a0_0 .var "light", 0 0;
v0x55d0e5b54160_0 .var "next_state", 1 0;
v0x55d0e5b54290_0 .net "reset", 0 0, v0x55d0e5b54600_0;  1 drivers
E_0x55d0e5b28f60 .event edge, v0x55d0e5b53fe0_0, v0x55d0e5aefcf0_0;
E_0x55d0e5b296b0 .event posedge, v0x55d0e5b54290_0, v0x55d0e5b53f20_0;
    .scope S_0x55d0e5b2d860;
T_0 ;
    %wait E_0x55d0e5b296b0;
    %load/vec4 v0x55d0e5b54290_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x55d0e5b53fe0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55d0e5b54160_0;
    %assign/vec4 v0x55d0e5b53fe0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55d0e5b2d860;
T_1 ;
Ewait_0 .event/or E_0x55d0e5b28f60, E_0x0;
    %wait Ewait_0;
    %load/vec4 v0x55d0e5b53fe0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d0e5b540a0_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55d0e5b54160_0, 0, 2;
    %jmp T_1.3;
T_1.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d0e5b540a0_0, 0, 1;
    %load/vec4 v0x55d0e5aefcf0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.4, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x55d0e5b54160_0, 0, 2;
    %jmp T_1.5;
T_1.4 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55d0e5b54160_0, 0, 2;
T_1.5 ;
    %jmp T_1.3;
T_1.1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d0e5b540a0_0, 0, 1;
    %load/vec4 v0x55d0e5aefcf0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.6, 8;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55d0e5b54160_0, 0, 2;
    %jmp T_1.7;
T_1.6 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x55d0e5b54160_0, 0, 2;
T_1.7 ;
    %jmp T_1.3;
T_1.3 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x55d0e5b2d6d0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d0e5b54490_0, 0, 1;
T_2.0 ;
    %delay 5, 0;
    %load/vec4 v0x55d0e5b54490_0;
    %inv;
    %store/vec4 v0x55d0e5b54490_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x55d0e5b2d6d0;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d0e5b54600_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d0e5b543d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d0e5b54600_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d0e5b543d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d0e5b543d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d0e5b543d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d0e5b543d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d0e5b54600_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d0e5b54600_0, 0, 1;
    %delay 10, 0;
    %vpi_call/w 3 51 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x55d0e5b2d6d0;
T_4 ;
    %vpi_call/w 3 56 "$monitor", "Time: %0d, Light: %b", $time, v0x55d0e5b54530_0 {0 0 0};
    %vpi_call/w 3 57 "$dumpfile", "light_controller_tb.vcd" {0 0 0};
    %vpi_call/w 3 58 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55d0e5b2d6d0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
